<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.votingdemo.dao.Vote_numberDao">
    <select id="getNumberById" parameterType="com.votingdemo.entity.Vote_number">
        SELECT COUNT(d_id=#{d_id}) FROM vote_number WHERE c_id=#{c_id}
    </select>
</mapper>